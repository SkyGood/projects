@CHARSET "UTF-8";
/*search form style*/
.sh-system-search{
	width: 800px;
	left: 400px;
	top:100px;
	margin:20px 0px 16px 80px;
	height: 32px;
}
.sh-system-search form{
	text-indent: 16px;
	font-size: 14px;
	height: 30px;
	line-height: 30px;
	float: left;
}
.sh-system-search a{
	float: right;
	height: 100%;
	line-height: 30px;
	font-size: 12px;
	color: #0097a7;
}
.sh-system-search form input{
	width: 180px;
	font-size: 12px;
	height: 22px;
}
.sh-system-search form input:last-child{
	width: 80px;
	cursor:pointer;
	border: none;
	color: white;
	background-color: #5cb85c;
	background-image: -webkit-gradient(linear,left top,left bottom,from(#5cb85c),to(#3d8b5f));
	
}
/*table of style*/
.container{
	width: 1000px;
	left: 400px;
	top:100px;
	margin:20px auto;
}
table,tr,td,a{
	margin: 0px;
	padding: 0px;
	border-collapse: collapse;
}
.table{
	width: 100%;
}
.table tr{
	/*border: 1px solid red;*/
	width: 100%;
	height: 34px;
	line-height: 34px;
	font-size: 15px;
	font-family: 微软雅黑;
	text-align: center;
	color: #333;
}
.table tr td{
	border:1px solid #ccc;
	text-indent: 8px;
	font-size: 13px;
	max-width: 300px;
}
.table tr td a{
	text-decoration: none;
	color: #259b24;
}
.table tr th{
	background: #4db6ac;
	color: #fefefe;
	border:1px solid white;
	border-top:1px solid #00aaa7;
}
.table tr th:first-child{
	border-left: 1px solid #00aaa7;
}
.table tr th:last-child{
	border-right: 1px solid #00aaa7;
}