.page-outer{
	width: 420px;
	height: 40px;
	margin-top: 20px;
	margin: 0px auto;
}
.page-normal {
	color:#efefef;
	text-align:center;
}
.page-normal a{
	float: left;
	width: 30px;
	height: 30px;
	line-height: 30px;
	text-decoration:none;
	font-weight: bolder;
}
.page-current {
	color:white;
	background:#0097a7;
}
.page-normal .page-prev{
	border:1px solid #ffe3c6;
	color:#ffe3c6;
	padding:5px 7px;
}
.page-next{
	border:1px solid #ffe3c6;
	color:#ffe3c6;
	padding:5px 7px;
}
