.page-outer{
	width: 420px;
	height: 40px;
	margin-top: 20px;
	margin: 0px auto;
}
.page-normal {
	color:#ff6500;
	text-align:center;
}
.page-normal a{
	border:1px solid #ff9600;
	color:#ff6500;
	float: left;
	width: 30px;
	height: 30px;
	line-height: 30px;
	text-decoration:none;
}

.page-current {
	border:1px solid #ff6500;
	color:#ff6500;
	background:#ffbe94;
}
.page-normal .page-prev{
	border:1px solid #ffe3c6;
	color:#ffe3c6;
	padding:5px 7px;
}

.page-next{
	border:1px solid #ffe3c6;
	color:#ffe3c6;
	padding:5px 7px;
}
